  },
  "dependencies": {
    "@radix-ui/react-slot": "1.0.0",
    "react": "18.2.0",
    "utils": "*"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@react-email/render": "*",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.3",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "babel-jest": "29.3.1",
    "eslint": "8.23.1",
    "eslint-config-custom": "*",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "ts-jest": "28.0.8",
    "tsconfig": "*",
    "tsup": "6.2.3",
    "typescript": "4.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
